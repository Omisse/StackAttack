config_version=5

[application]

config/name="StackAttack"
run/main_scene="res://UI/UIScenes/MainMenuUI.tscn"
run/disable_stdout=true
run/disable_stderr=true
config/features=PackedStringArray("4.3", "GL Compatibility")

[audio]

buses/default_bus_layout="res://Audio/AudioBus.tres"
driver/output_latency=100
driver/output_latency.web=100

[autoload]

Audio="*res://Audio/Audio.tscn"
Yandex="*res://YandexSDK/YandexGamesScript.gd"
ScoreStorage="*res://YandexSDK/SDKVars.gd"

[display]

